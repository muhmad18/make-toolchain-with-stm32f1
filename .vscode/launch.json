  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceRoot}",
      "executable": "./build/maketest.elf",
      "name": "Cortex Debug",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "device": "STM32F103C8",
      "configFiles": [
        "C:/dev/tools/openocd/share/openocd/scripts/interface/stlink-v2.cfg",
        "C:/dev/tools/openocd/share/openocd/scripts/target/stm32f1x.cfg"
      ],
      "searchDir": [],
      "runToEntryPoint": "main",
      "showDevDebugOutput": "none",
      "svdFile": "./STM32F103.svd"
    }
  ]
}
